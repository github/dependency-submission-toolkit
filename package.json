{
  "name": "@github/dependency-submission-toolkit",
  "version": "2.0.4",
  "description": "A TypeScript library for creating dependency snapshots.",
  "homepage": "https://github.com/github/dependency-submission-toolkit",
  "bugs": {
    "url": "https://github.com/github/dependency-submission-toolkit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/github/dependency-submission-toolkit.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "workspaces": [
    "example"
  ],
  "scripts": {
    "all": "npm run build && npm run check && npm run test",
    "build": "tshy",
    "check": "biome check",
    "fix": "biome check --fix",
    "prepare": "npm run build",
    "test": "vitest run src",
    "test:watch": "vitest watch src"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "@octokit/webhooks-types": "^7.5.1",
    "packageurl-js": "^1.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@tsconfig/node20": "^20.1.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^20.14.10",
    "tshy": "^3.0.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.1"
  },
  "tshy": {
    "exclude": [
      "./src/*.test.ts"
    ],
    "exports": {
      ".": "./src/index.ts"
    }
  }
}
